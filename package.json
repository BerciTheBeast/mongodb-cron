{
  "name": "mongodb-cron",
  "version": "0.1.0",
  "description": "MongoDB collection as crontab",
  "main": "./dist/index.js",
  "scripts": {
    "clean": "rm -Rf ./dist",
    "prebuild": "npm run clean; mkdir -p ./dist",
    "build": "babel ./src --out-dir ./dist --copy-files",
    "preexample": "npm run build",
    "example": "npm run build; node ./example",
    "pretest": "npm run build",
    "test": "ava"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xpepermint/mongodb-cron.git"
  },
  "keywords": [
    "mongo",
    "mongodb",
    "database",
    "nosql",
    "cron",
    "schedule",
    "scheduling",
    "queue",
    "job",
    "collection",
    "capped"
  ],
  "ava": {
    "files": [
      "./dist/__tests__/*.js",
      "./dist/**/__tests__/*.js"
    ],
    "concurrency": 1,
    "failFast": true
  },
  "author": "Xpepermint (Kristijan Sedlak)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/xpepermint/mongodb-cron/issues"
  },
  "homepage": "https://github.com/xpepermint/mongodb-cron#readme",
  "devDependencies": {
    "ava": "0.15.2",
    "babel-cli": "6.11.4",
    "babel-preset-node5": "11.1.0"
  },
  "dependencies": {
    "es6-sleep": "2.0.2",
    "later": "1.2.0",
    "moment": "2.14.1",
    "mongodb": "2.2.5"
  }
}
