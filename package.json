{
  "name": "mongodb-cron",
  "version": "0.7.2",
  "description": "MongoDB collection as crontab",
  "main": "./dist/index.js",
  "scripts": {
    "clean": "rm -Rf ./dist",
    "prebuild": "npm run clean; mkdir -p ./dist",
    "build": "babel ./src --out-dir ./dist --copy-files",
    "example": "babel-node ./example",
    "prepublish": "npm run build",
    "pretest": "npm run build",
    "test": "ava",
    "speedtest": "babel-node ./speedtest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xpepermint/mongodb-cron.git"
  },
  "keywords": [
    "mongo",
    "mongodb",
    "database",
    "nosql",
    "cron",
    "schedule",
    "scheduling",
    "queue",
    "job",
    "jobs",
    "collection",
    "collections",
    "capped"
  ],
  "ava": {
    "files": [
      "./tests/*.js",
      "./tests/**/*.js"
    ],
    "concurrency": 1,
    "failFast": true
  },
  "author": "Xpepermint (Kristijan Sedlak)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/xpepermint/mongodb-cron/issues"
  },
  "homepage": "https://github.com/xpepermint/mongodb-cron#readme",
  "devDependencies": {
    "ava": "^0.19.1",
    "babel-cli": "^6.24.1",
    "babel-preset-node5": "^12.0.1",
    "mongodb": "^2.2.26"
  },
  "peerDependencies": {
    "mongodb": "^2.2.26"
  },
  "dependencies": {
    "dot-object": "^1.5.4",
    "es6-sleep": "^2.0.2",
    "later": "^1.2.0",
    "moment": "^2.18.1"
  }
}
