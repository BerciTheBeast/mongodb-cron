{
  "name": "mongodb-cron",
  "version": "0.3.1",
  "description": "MongoDB collection as crontab",
  "main": "./dist/index.js",
  "scripts": {
    "clean": "rm -Rf ./dist",
    "prebuild": "npm run clean; mkdir -p ./dist",
    "build": "babel ./src --out-dir ./dist --copy-files",
    "example": "babel-node ./example",
    "prepublish": "npm run build",
    "pretest": "npm run build",
    "test": "ava"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xpepermint/mongodb-cron.git"
  },
  "keywords": [
    "mongo",
    "mongodb",
    "database",
    "nosql",
    "cron",
    "schedule",
    "scheduling",
    "queue",
    "job",
    "jobs",
    "collection",
    "collections",
    "capped",
    "redis",
    "ioredis"
  ],
  "ava": {
    "files": [
      "./tests/*.js",
      "./tests/**/*.js"
    ],
    "concurrency": 1,
    "failFast": true
  },
  "author": "Xpepermint (Kristijan Sedlak)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/xpepermint/mongodb-cron/issues"
  },
  "homepage": "https://github.com/xpepermint/mongodb-cron#readme",
  "devDependencies": {
    "ava": "0.16.x",
    "babel-cli": "6.14.x",
    "babel-preset-node5": "11.1.x",
    "mongodb": "2.2.x"
  },
  "peerDependencies": {
    "ioredis": "2.3.x",
    "mongodb": "2.2.x"
  },
  "dependencies": {
    "dot-object": "1.5.2",
    "es6-sleep": "2.0.x",
    "later": "1.2.x",
    "moment": "2.15.x"
  }
}
